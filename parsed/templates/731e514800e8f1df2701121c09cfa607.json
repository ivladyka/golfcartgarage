{
  "components/blog/post": "<article class=\"blog blog-single-post-page\">\r\n    <div class=\"blog-post\">\r\n        <header class=\"blog-header\">\r\n            <h1 class=\"blog-title\">\r\n                <a href=\"{{post.url}}\">{{post.title}}</a>\r\n            </h1>\r\n            <!-- <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p> -->\r\n        </header>\r\n    </div>\r\n\r\n \r\n        <div class=\"blog-post-figure hi\">\r\n        {{#if post.thumbnail}}\r\n          <figure class=\"blog-thumbnail\" style=\"background-image:url({{getImage post.thumbnail}})\">\r\n            <!--\r\n             <a href=\"{{post.url}}\"><img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\r\n             </a>-->\r\n            <!-- -->\r\n        </figure>\r\n        {{/if}}\r\n    </div>\r\n   \r\n    <div class=\"blog-post-body\">\r\n\r\n        <div class=\"blog-post\">\r\n            {{#if post.body}}\r\n            {{{post.body}}}\r\n            {{else}}\r\n            {{{post.summary}}}\r\n            {{#if post.show_read_more}}\r\n            &hellip; <a href=\"{{url}}\">read more</a>\r\n            {{/if}}\r\n            {{/if}}\r\n        </div>\r\n\r\n\r\n        {{#if show_tags}}\r\n        {{#if post.tags}}\r\n        <ul class=\"tags\">\r\n            {{#each post.tags}}\r\n            <li class=\"tag\">\r\n                <a href=\"{{url}}\">#{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{/if}}\r\n        {{/if}}\r\n\r\n        {{>components/common/share}}\r\n\r\n    </div>\r\n</article>\r\n",
  "components/common/share": "{{#if settings.add_this.buttons}}\n    <div>\n        <ul class=\"socialLinks\">\n            {{#each settings.add_this.buttons}}\n                {{#if service '===' 'google_plusone'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\"\n                        class=\"google-plus icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-google-plus\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'facebook'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\"\n                        class=\"facebook icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-facebook\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'email'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n                        class=\"email icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-envelope\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'print'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a class=\"print icon\" href=\"javascript:;\" onclick=\"window.print()\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-print\"></use>\n                        </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'twitter'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-twitter\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'linkedin'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\"\n                        class=\"linkedin icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-linkedin\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'pinterest'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest icon\">\n                            <svg class=\"icon-social\">\n                                <use xlink:href=\"#icon-pinterest\"></use>\n                            </svg>\n                        </a>\n                    </li>\n                {{/if}}\n                \n                {{#if service '===' 'facebook_like'}}\n                    <li class=\"socialLinks-item socialLinks-item--{{service}}\">\n                        <div class=\"fb-like\"\n                            data-href=\"{{../../url}}\"\n                            data-layout=\"button_count\"\n                            data-action=\"like\"\n                            data-show-faces=\"false\">\n                        </div>\n                    </li>\n                {{/if}}\n            {{/each}}\n        </ul>\n    </div>\n{{/if}}\n"
}
