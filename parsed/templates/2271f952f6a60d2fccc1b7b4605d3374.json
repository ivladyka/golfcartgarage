{
  "components/blog/blog-list-post": "<article class=\"blog\">\n\n\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            <h1 class=\"blog-title\">\n                <a href=\"{{post.url}}\">{{post.title}}</a>\n            </h1>\n           <!-- <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p> -->\n        </header>\n      \n    <a href=\"{{post.url}}\"> <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\" style='background-image:url({{getImage post.thumbnail}})'>\n              \n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n            \n            </figure>\n        {{/if}}\n    </div>\n       </a>\n\n\n      \n\n    </div>\n</article>\n\n"
}
