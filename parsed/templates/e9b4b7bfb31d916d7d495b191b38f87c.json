{
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters sidebarBlock\">\n    <h5 class=\"sidebarBlock-heading\">\n        {{lang 'search.faceted.selected.title'}}\n    </h5>\n\n    {{#if this.items}}\n<ul class=\"inlineList inlineList--labels\">\n    {{#each this.items}}\n\n    {{#if param_name '===' 'model'}}\n    <div style=\"display:none\" class=\"ysw-hidden-filter-selected\" data-filter-name=\"model\">{{ title }}</div>\n    <!--{{#if title '===' 'DS'}}-->\n    <!--<div id=\"year_id\" style=\"display:none\" class=\"ysw-hidden-filter-selected\" data-filter-name=\"year\">2000 (January-June)</div>-->\n    <!--{{/if}}-->\n    {{else if param_name '===' 'make'}}\n    <div style=\"display:none\" class=\"ysw-hidden-filter-selected\" data-filter-name=\"make\">{{ title }}</div>\n    {{else if param_name '===' 'year'}}\n    <div style=\"display:none\" class=\"ysw-hidden-filter-selected\" data-filter-name=\"year\">{{ title }}</div>\n    {{else}}\n\n    <li>\n        <a href=\"{{ url }}\"\n           class=\"facetLabel\"\n           rel=\"nofollow\"\n           data-faceted-search-facet>\n            {{#if facet '===' 'rating'}}\n            {{lang 'search.faceted.selected.rating-label' rating=title}}\n            {{else}}\n            {{ title }}\n            {{/if}}\n\n            <svg class=\"icon\">\n                <use xlink:href=\"#icon-close\" />\n            </svg>\n        </a>\n    </li>\n\n    {{/if}}\n    {{/each}}\n</ul>\n    {{else}}\n        {{lang 'search.faceted.selected.no-filters'}}\n    {{/if}}\n\n    {{#if remove_all_url}}\n       <!-- <a href=\"{{ remove_all_url }}\" data-faceted-search-facet>\n            {{lang 'search.faceted.selected.clear-all'}}\n        </a> -->\n    {{/if}}\n</div>\n<script>\n    function getCookie (name) {\n\t\tvar nameEQ = name + \"=\";\n\t\tvar ca = document.cookie.split(';');\n\t\tfor(var i=0;i < ca.length;i++) {\n\t\t\tvar c = ca[i];\n\t\t\twhile (c.charAt(0)==' ') c = c.substring(1,c.length);\n\t\t\tif (c.indexOf(nameEQ) == 0) return decodeURIComponent(c.substring(nameEQ.length,c.length));\n\t\t}\n\t\treturn null;\n    }\n    if(getCookie('ysw_fa_year')) {\n        var gtc = getCookie('ysw_fa_year');\n\t\t\tif(gtc == '2000 (January-June)' || gtc == '2000 (July-December)'){\n\t\t\t    document.getElementById(\"year_id\").innerHTML = gtc;\n\t\t\t}else{\n\t\t\t    document.getElementById(\"year_id\").remove();\n\t\t\t}\n\t}\n</script>\n"
}
