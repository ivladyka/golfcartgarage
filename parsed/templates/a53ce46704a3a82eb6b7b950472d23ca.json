{
  "components/carousel": "<section class=\"heroCarousel\"\r\n         data-slick='{\r\n        \"arrows\": {{arrows}},\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 1,\r\n        \"slidesToScroll\": 1,\r\n        \"autoplay\": true,\r\n        \"autoplaySpeed\": {{carousel.swap_frequency}},\r\n        \"lazyLoad\": \"anticipated\"\r\n    }'>\r\n    {{#each carousel.slides}}\r\n    {{#if url}}\r\n    <div>\r\n        <a href=\"{{url}}\">\r\n            -\r\n            {{/if}}\r\n            <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\r\n                <div class=\"heroCarousel-image-wrapper\" {{#if image_height}} {{#if image_width}} style=\"background-size:cover;    background-position: center; background-image:url('{{image}}'); height: {{multiply (divide image_height image_width) 100}}vw; max-height: {{multiply (divide image_height image_width) 800}}px;\" {{/if}} {{/if}}>\r\n\r\n                </div>\r\n                {{> components/carousel-content show_background=true}}\r\n                {{#if heading}}\r\n                {{> components/carousel-content show_background=true}}\r\n                {{else}}\r\n                {{#if text}}\r\n                {{> components/carousel-content show_background=true}}\r\n                {{else}}\r\n                {{#if button_text}}\r\n                {{> components/carousel-content show_background=false}}\r\n                {{/if}}\r\n                {{/if}}\r\n                {{/if}}\r\n            </div>\r\n            {{#if url}}\r\n        </a>\r\n    </div>\r\n    {{/if}}\r\n    {{/each}}\r\n</section>\r\n",
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\r\n    <p class=\"heroCarousel-title\">{{{heading}}}</p>\r\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\r\n    {{#if button_text}}\r\n<a href=\"{{url}}\">\r\n    <span class=\"heroCarousel-action button button--primary button--large\">{{button_text}}</span>\r\n</a>\r\n        {{/if}}\r\n</div>\r\n"
}
